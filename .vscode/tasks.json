{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "npm",
            "script": "compile",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "isBackground": true,
            "presentation": {
                "reveal": "never"
            },
            "problemMatcher": "$tsc-watch"
        },
        {
            "type": "npm",
            "script": "lint",
            "problemMatcher": "$eslint-stylish"
        },
        {
            "type": "npm",
            "script": "all",
            "problemMatcher": [
                "$eslint-compact"
            ]
        },
        {
            "type": "npm",
            "script": "build-grammars",
            "problemMatcher": []
        },
        {
            "type": "npm",
            "script": "watch-grammars",
            "problemMatcher": [],
            "isBackground": true
        },
        {
            "type": "npm",
            "script": "verify-test-uses-extension-bundle",
            "problemMatcher": []
        }
    ]
}
